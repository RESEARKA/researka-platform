# Sample environment configuration
# Copy this file to .env.local for development or .env.production for production

# CDN URL for static assets (leave empty to serve assets from the same domain)
CDN_URL=https://cdn.researka.org/

# API URL
API_URL=https://api.researka.org/api

# Analytics ID (if using analytics)
ANALYTICS_ID=

# Feature flags
ENABLE_DARK_MODE=true
ENABLE_NOTIFICATIONS=true
